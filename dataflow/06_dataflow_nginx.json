{
	"name": "06_dataflow_nginx",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "accessLog",
						"type": "DatasetReference"
					},
					"name": "nginxStream"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "accessTable",
						"type": "DatasetReference"
					},
					"name": "JSONData"
				}
			],
			"transformations": [
				{
					"name": "selectColumns"
				},
				{
					"name": "derivedTimeColumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> nginxStream",
				"nginxStream select(mapColumn(",
				"          ch_RemoteAddr = Column_1,",
				"          ch_TimeLocal = Column_4,",
				"          ch_Request = Column_6,",
				"          int_Status = Column_7,",
				"          int_Bytes = Column_8",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectColumns",
				"selectColumns derive(ch_TimeLocal = substring(ch_TimeLocal,2,length(ch_TimeLocal)-1)) ~> derivedTimeColumn",
				"derivedTimeColumn sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ch_RemoteAddr as string,",
				"          ch_TimeLocal as string,",
				"          ch_Request as string,",
				"          int_Status as integer,",
				"          Bytes as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          ch_RemoteAddr,",
				"          ch_TimeLocal,",
				"          ch_Request,",
				"          int_Status,",
				"          int_Bytes",
				"     )) ~> JSONData"
			]
		}
	}
}